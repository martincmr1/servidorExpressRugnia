{
    "version": 2,
    "builds": [
      { "src": "sources/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "sources/index.js"
      }
    ],
    "frameworks": {
      "node": {
        "buildCommand": "npm install && npm run build", 
        "outputDirectory": "sources"
      }
    }
  }
  